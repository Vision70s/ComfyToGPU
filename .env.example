# Gemini API Configuration
# Get your API key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Security (optional - for production use)
API_ACCESS_KEYS=secret-key-1,secret-key-2
SKIP_AUTH=false

# RunPod Configuration
# Get your API key and endpoint from: https://www.runpod.io/console/serverless
RUNPOD_API_KEY=your_runpod_api_key_here
RUNPOD_ENDPOINT_ID=your_endpoint_id_here
RUNPOD_API_URL=https://api.runpod.ai/v2

# Server Configuration
NODE_ENV=development
PORT=3000

# Production CORS (comma-separated list of allowed origins)
ALLOWED_ORIGINS=https://yourdomain.com,http://localhost:3000

# Proxy Configuration (if needed for Gemini API in your region)
# HTTP_PROXY=socks5://127.0.0.1:1085
# HTTPS_PROXY=socks5://127.0.0.1:1085
